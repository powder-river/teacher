<p id="notice"><%= notice %></p>

<h1>Listing Parents</h1>

<%= link_to 'New Parent', new_parent_path %>

<% %>
  <table>
    <thead>
      <tr>
        <th>Student name</th>
        <th>Parent name</th>
        <th>Parent email</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @list_by_educator.each do |parent| %>
        <tr>
        <td><%= parent.student_name %></td>
          <td><%= parent.parent_name %></td>
          <td><%= parent.parent_email %></td>
          <td><%= link_to 'Show', parent %></td>
          <td><%= link_to 'Edit', edit_parent_path(parent) %></td>
          <td><%= link_to 'Destroy', parent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

<br>
